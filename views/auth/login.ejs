<div id = "loginHeader">Welcome back! Login below</div>
<br>
<br>
<br>
<form method = "POST" action = "/auth/login">
	<div class = "form-group">
		<label>Email</label>
		<input type = "text" name = "email" class = "form-control" placeholder = "Email goes here">
	</div>
	<div class = "form-group">
		<label>Password</label>
		<input type = "password" name = "password" class = "form-control" placeholder = "Secret things go here">
	</div>
	<input type = "submit" value = "Log me in!" class = "btn btn-success" id = "logMeInButton">
</form>
